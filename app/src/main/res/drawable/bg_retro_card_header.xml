<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="schemas.android.com">
    <item
        android:left="6dp"
        android:top="6dp">
        <shape android:shape="rectangle">
            <solid android:color="#000000" />
            <corners android:radius="12dp" />
        </shape>
    </item>

    <item
        android:right="6dp"
        android:bottom="6dp">
        <shape android:shape="rectangle">
            <solid android:color="#A2D2FF" />
            <stroke
                android:width="3dp"
                android:color="#000000" />
            <corners android:radius="12dp" />
        </shape>
    </item>
</layer-list>
